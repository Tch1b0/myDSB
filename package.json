{
    "name": "my-dsb",
    "version": "0.0.1",
    "private": true,
    "description": "A DSBmobile-mobile alternative",
    "prettier": "@tch1b0/prettier-config",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build:android": "ionic cap build android",
        "build:ios": "ionic cap build ios",
        "build:all": "npm run build:android && npm run build:ios",
        "test": "cypress run",
        "test:gui": "cypress open",
        "test:main": "cypress run main",
        "resources": "capacitor-resources -p android,ios",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@capacitor/android": "3.3.3",
        "@capacitor/app": "1.0.7",
        "@capacitor/core": "3.3.3",
        "@capacitor/haptics": "1.1.3",
        "@capacitor/ios": "3.3.3",
        "@capacitor/keyboard": "1.2.0",
        "@capacitor/status-bar": "1.0.6",
        "@ionic/storage": "^3.0.6",
        "@ionic/vue": "^5.4.0",
        "@ionic/vue-router": "^5.4.0",
        "@tch1b0/prettier-config": "^1.0.0",
        "@types/node": "^16.11.6",
        "capacitor-resources": "^2.0.5",
        "core-js": "^3.6.5",
        "dsbmobile": "^1.0.9",
        "vue": "^3.2.1",
        "vue-router": "^4.0.0-0",
        "vuex": "^4.0.0"
    },
    "devDependencies": {
        "@capacitor/cli": "3.3.3",
        "@types/cypress": "^1.1.3",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "^5.0.0-beta.4",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0-0",
        "@vue/eslint-config-typescript": "^5.0.2",
        "@vue/test-utils": "^2.0.0-0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0-0",
        "typescript": "^4.3.5"
    }
