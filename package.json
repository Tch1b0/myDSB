{
    "name": "my-dsb",
    "version": "0.1.0",
    "private": true,
    "description": "A DSBmobile-mobile alternative",
    "license": "GPL-3.0",
    "prettier": "@tch1b0/prettier-config",
    "scripts": {
        "serve": "vue-cli-service serve -p=8080",
        "build": "vue-cli-service build",
        "build:android": "ionic cap build android",
        "build:ios": "ionic cap build ios",
        "build:all": "npm run build:android && npm run build:ios",
        "test:unit": "mocha --require ts-node/register ./test/*.spec.ts",
        "test:gui": "cypress open",
        "test:main": "cypress run main",
        "resources": "capacitor-resources -p android,ios",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@capacitor/android": "3.3.4",
        "@capacitor/app": "1.0.7",
        "@capacitor/core": "3.3.4",
        "@capacitor/haptics": "1.1.3",
        "@capacitor/ios": "3.3.4",
        "@capacitor/keyboard": "1.2.0",
        "@capacitor/local-notifications": "1.0.9",
        "@capacitor/status-bar": "1.0.6",
        "@ionic/storage": "3.0.6",
        "@ionic/vue": "6.0.1",
        "@ionic/vue-router": "6.0.1",
        "@tch1b0/prettier-config": "1.0.0",
        "@types/node": "16.11.19",
        "capacitor-resources": "2.0.5",
        "core-js": "3.20.2",
        "dsbmobile": "1.0.9",
        "vue": "3.2.26",
        "vue-router": "4.0.12",
        "vuex": "4.0.2"
    },
    "devDependencies": {
        "@capacitor/cli": "3.3.4",
        "@types/chai": "4.3.0",
        "@types/cypress": "1.1.3",
        "@types/mocha": "9.0.0",
        "@typescript-eslint/eslint-plugin": "2.34.0",
        "@typescript-eslint/parser": "2.34.0",
        "@vue/cli-plugin-babel": "4.5.15",
        "@vue/cli-plugin-eslint": "4.5.15",
        "@vue/cli-plugin-router": "4.5.15",
        "@vue/cli-plugin-typescript": "4.5.15",
        "@vue/cli-plugin-vuex": "5.0.0-rc.1",
        "@vue/cli-service": "4.5.15",
        "@vue/compiler-sfc": "3.2.26",
        "@vue/eslint-config-typescript": "5.1.0",
        "@vue/test-utils": "2.0.0-rc.18",
        "chai": "4.3.4",
        "eslint": "6.8.0",
        "eslint-plugin-vue": "7.20.0",
        "mocha": "9.1.4",
        "ts-node": "10.4.0",
        "typescript": "4.5.4"
    }
}
